{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "ES2022",
    "lib": ["ES2023"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "declaration": true,
    "outDir": "./dist/node",
    "composite": true
  },
  "include": ["vite.config.ts"]
}
